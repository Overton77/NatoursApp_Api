{
  "name": "natours",
  "version": "1.0.0",
  "description": "a tour booking application written on top of node.js",
  "source": "./public/js/index.js",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "browserslist": "> 0.25%",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": " PORT=1234 parcel serve ./public/js/index.js --dist-dir ./public/dist/js",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/dist/js",
    "build-and-start": "npm run build:js && nodemon server.js"
  },
  "author": "Overton",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@mapbox/mapbox-sdk": "^0.15.3",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.13.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "pug": "^3.0.2",
    "sharp": "^0.33.2",
    "slugify": "^1.6.6",
    "stripe": "^14.19.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "parcel": "^2.11.0",
    "prettier": "^3.0.3"
